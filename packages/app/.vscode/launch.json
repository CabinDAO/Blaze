

{
    "version": "0.2.0",
    "configurations": [
        {
          "name": "Next: Launch",
          "type": "firefox",
          "request": "launch",
          "reAttach": true,
          "url": "http://localhost:3000",
          "webRoot": "${workspaceFolder}",
          "profile": "dev-edition-default",
          "pathMappings": [
            {
                "/home/pi/Dev/client-projects/dao-camp/packages/app": "${workspaceRoot}/app",
            }
          ]
        },
        {
          "type": "node",
          "request": "launch",
          "name": "Next: Node",
          "runtimeExecutable": "${workspaceFolder}/node_modules/next/dist/bin/next",
          "port": 9230,
          "console": "integratedTerminal",
          "env": {
            "NODE_OPTIONS": "--inspect=9230"
          }
        }],
      "compounds": [
        {
          "name": "Next: Full Launch",
          "configurations": ["Next: Node", "Next: Launch"]
        },
        {
          "name": "Next: Full Attach",
          "configurations": ["Next: Node", "Next: Attach"]
        }
      ]
      }
  